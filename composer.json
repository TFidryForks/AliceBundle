{
    "name": "theofidry/alice-bundle",
    "description": "Symfony2 Bundle to manage fixtures with Alice and Faker.",
    "keywords": ["Symfony", "Fixture", "ORM", "Alice", "Faker"],
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Baldur Rensch",
            "email": "brensch@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "nelmio/alice": "2.0",
        "symfony/finder": "^2.7"
    },
  "require-dev": {
        "doctrine/orm": "~2.4",
        "doctrine/doctrine-bundle": "~1.2",
        "doctrine/doctrine-fixtures-bundle": "~2.2",
        "fabpot/php-cs-fixer": "~2.0@dev",
        "phpunit/phpunit": "^4.7",
        "symfony/console": "~2.1",
        "symfony/framework-bundle": "~2.1",
        "symfony/yaml": "~2.1",
        "symfony/validator": "~2.1",
        "theofidry/psysh-bundle": "~1.1",
        "symfony/expression-language": "^2.7"
    },
    "suggest": {
        "doctrine/orm": "To use Doctrine ORM",
        "doctrine/doctrine-bundle": "To use Doctrine with Symfony",
        "doctrine/doctrine/data-fixtures": "To use Doctrine fixtures loader"
    },
    "autoload": {
        "psr-4": { "Hautelook\\AliceBundle\\": "" }
    }
}
